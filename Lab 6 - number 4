#include <stdio.h>

int main()
{
    int array_num[10] = {};
    int array_num2[10] = {};
    int array_sum[10];
    int max=0, min=100000;
    
    for(int i = 0; i<10; i++){
        array_sum[i] = array_num[i] + array_num2[i];
    }
    
    min = array_sum[0];
    
    for (int i =0; i<10; i++){
        if(array_sum[i]>max){
            max = array_sum[i];
        }
        if(array_sum[i]<min){
            min = array_sum[i];
        }
        printf("Sum %d is equal to %d\n", i+1, array_sum[i]);
    }
    
    printf("The max sum is %d and the min sum is %d", max, min);
}
