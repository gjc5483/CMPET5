
//This program is to display “your Name, ID, Department and University name”
// Including a library file that contains all the declarations of std functions for input & output 
#include <stdio.h>

int main()
{
    char name[] = "Gabby Clemente";
    char university[] = "Penn State Behrend";
    long ID = 964487597;
    char dept[] = "ECET";
    printf("\n my name: %s", name);
    printf("\n my university: %s", university);
    printf("\n department: %s", dept);
    printf("\n ID: %ld", ID);

    return 0;
}
